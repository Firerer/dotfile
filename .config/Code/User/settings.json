{
  "editor.fontSize": 20,
  "[html]": {},
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {},
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[rust]": {
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "git.autofetch": true,
  "[haskell]": {},
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "editor.wordWrap": "on",
  "editor.rulers": [],
  "python.languageServer": "Jedi",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "rust-client.revealOutputChannelOn": "error",
  "rust-client.updateOnStartup": true,
  "rust.racer_completion": false,
  "workbench.colorCustomizations": {
    "[Material Theme Darker]": {
      "activityBarBadge.background": "#5C6BC0",
      "activityBar.activeBorder": "#5C6BC0",
      "list.activeSelectionForeground": "#5C6BC0",
      "list.inactiveSelectionForeground": "#5C6BC0",
      "list.highlightForeground": "#5C6BC0",
      "scrollbarSlider.activeBackground": "#5C6BC050",
      "editorSuggestWidget.highlightForeground": "#5C6BC0",
      "textLink.foreground": "#5C6BC0",
      "progressBar.background": "#5C6BC0",
      "pickerGroup.foreground": "#5C6BC0",
      "tab.activeBorder": "#5C6BC0",
      "notificationLink.foreground": "#5C6BC0",
      "editorWidget.resizeBorder": "#5C6BC0",
      "editorWidget.border": "#5C6BC0",
      "settings.modifiedItemIndicator": "#5C6BC0",
      "settings.headerForeground": "#5C6BC0",
      "panelTitle.activeBorder": "#5C6BC0",
      "breadcrumb.activeSelectionForeground": "#5C6BC0",
      "menu.selectionForeground": "#5C6BC0",
      "menubar.selectionForeground": "#5C6BC0",
      "editor.findMatchBorder": "#5C6BC0",
      "selection.background": "#5C6BC040",
      "statusBarItem.remoteBackground": "#5C6BC0"
    },
    "[Material Theme Darker High Contrast]": {
      "activityBarBadge.background": "#F44336",
      "activityBar.activeBorder": "#F44336",
      "list.activeSelectionForeground": "#F44336",
      "list.inactiveSelectionForeground": "#F44336",
      "list.highlightForeground": "#F44336",
      "scrollbarSlider.activeBackground": "#F4433650",
      "editorSuggestWidget.highlightForeground": "#F44336",
      "textLink.foreground": "#F44336",
      "progressBar.background": "#F44336",
      "pickerGroup.foreground": "#F44336",
      "tab.activeBorder": "#F44336",
      "notificationLink.foreground": "#F44336",
      "editorWidget.resizeBorder": "#F44336",
      "editorWidget.border": "#F44336",
      "settings.modifiedItemIndicator": "#F44336",
      "settings.headerForeground": "#F44336",
      "panelTitle.activeBorder": "#F44336",
      "breadcrumb.activeSelectionForeground": "#F44336",
      "menu.selectionForeground": "#F44336",
      "menubar.selectionForeground": "#F44336",
      "editor.findMatchBorder": "#F44336",
      "selection.background": "#F4433640",
      "statusBarItem.remoteBackground": "#F44336"
    }
  },
  "gitlens.defaultDateFormat": null,
  "gitlens.blame.avatars": false,
  "kite.showWelcomeNotificationOnStartup": false,
  "python.testing.pytestEnabled": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "problems.showCurrentInStatus": true,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "files.eol": "\n",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.minimap.enabled": false,
  "editor.suggest.filterGraceful": false,
  "workbench.colorTheme": "Catppuccin Mocha",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  // vim settings
  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "vim.autoindent": true,
  "vim.surround": true,
  "vim.leader": "<space>",
  "vim.smartRelativeLine": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["g", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": [" "],
      "commands": ["whichkey.show"]
    }
  ],
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-b>": false
  },
  "vim.argumentObjectClosingDelimiters": [")", "]", ">"],
  "vim.argumentObjectOpeningDelimiters": [
    "(",
    "[",
    "<"
  ],
  "workbench.editor.limit.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "files.associations": {
    "*.log.*": "log"
  },
  "vim.highlightedyank.enable": true,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.easymotion": true,
  "whichkey.bindingOverrides": [
    {
      "keys": " ",
      "name": "Open project file",
      "type": "command",
      "command": "workbench.action.quickOpen"
    },
    {
      "keys": "o",
      "name": "+Open",
      "type": "bindings",
      "bindings": [
        {
          "keys": "s",
          "name": "Open settings",
          "type": "command",
          "command": "workbench.action.openSettings"
        }
      ]
    },
    {
      "keys": "t.z",
      "name": "Toggle zen mode",
      "type": "command",
      "command": "workbench.action.toggleZenMode"
    }
  ],
  "git.confirmSync": false,
  "editor.fontFamily": "'Hack Nerd Font', mononoki, unifont, monospace",
  "terminal.external.linuxExec": "alacritty",
  "workbench.iconTheme": "material-icon-theme",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.autoSave": "onFocusChange",
  "deno.enable": true,
  "editor.tabCompletion": "on",
  "javascript.suggest.completeFunctionCalls": true,
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": 1,
  "explorer.confirmDelete": false,
  "editor.suggest.insertMode": "replace",
  "deno.suggest.completeFunctionCalls": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false
}