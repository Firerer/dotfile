[user]
	email = liudi12631@gmail.com
	name = Di Liu
[alias]
	uncommit = reset HEAD^
  dft = difftool
[core]
	autocrlf = input
[push]
  followTags = true

# https://difftastic.wilfred.me.uk/git.html
[diff]
	tool = difftastic
[difftool]
  prompt = false
[difftool "difftastic"]
  cmd = difft "$LOCAL" "$REMOTE"
[pager]
  difftoll = true

[init]
	defaultBranch = main
[http]
	lowSpeedLimit = 1000
	lowSpeedTime = 600
